{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ] ,
    "redirects": [ {
      "source" : "/display/:space*",
      "destination" : "https://apereo.atlassian.net/wiki/spaces/:space*",
      "type" : 301
    }, {
      "source" : "/**",
      "destination" : "https://apereo.atlassian.net/wiki/",
      "type" : 301
    } ]
  }
}
